  "scripts": {
    "build:local-assets": "NODE_ENV=production bernie webpack --use-localhost",
    "build": "NODE_ENV=production bernie webpack",
    "clean:generated": "rimraf src/common/__generated__",
    "clean": "rimraf static && rimraf build && rimraf coverage && rimraf logs && rimraf dist",
    "dev:local-bff": "API_URL=http://localhost:8081/ yarn dev",
    "dev": "NODE_ENV=development bernie go --random-tpids 70201",
    "eslint": "echo 'Running eslint...' && eslint-cache --ext js,jsx,json .",
    "fake-bff": "bff-offline",
    "graphql": "yarn clean:generated && bernie graphql-schema && bernie graphql-client",
    "lint": "npm run stylelint && npm run eslint && npm run tslint && type-coverage",
    "nuke": "yarn clean && rimraf node_modules",
    "offline": "npm run fake-bff | API_URL=http://localhost:1337 CONTEXT_URL=http://localhost:1337/hotel-shop/api/context NODE_ENV=development bernie go",
    "perf-benchmark": "yarn build:local-assets && jest -c jest.config.perf.js",
    "postbuild": "gulp upload",
    "postprod": "bernie upload",
    "poststart:test": "NODE_ENV=test bernie go",
    "prebuild": "yarn clean",
    "predev": "./tasks/yarn-check.sh && yarn clean",
    "preprod": "yarn clean && bernie generate-build-info",
    "prestart:test": "yarn clean",
    "pretest": "npm run test:lockfile && npm run clean && npm run lint --silent",
    "prettify": "echo 'Prettifying...' && prettier --config .prettierrc.json --write",
    "prod": "NODE_ENV=production bernie webpack",
    "rebuild": "./tasks/yarn-check.sh && yarn nuke && yarn install && yarn graphql",
    "reorder": "node_modules/import-reorder/reorder.js",
    "start:local-assets": "bernie go --use-localhost",
    "start:test": "NODE_ENV=test bernie webpack && gulp upload",
    "start": "bernie go",
    "stylelint": "echo 'Running stylelint...' && stylelint 'src/common/stylesheets/**/*.scss' -s scss",
    "test:bundles:es5": "jest --coverage=false --testMatch='<rootDir>/test/client/es5-target-validation.ts' --verbose",
    "test:bundles:size": "yarn size-limit",
    "test:bundles": "NODE_ENV=production bernie webpack --only-client && npm run test:bundles:size && npm run test:bundles:es5",
    "test:graphql": "jest --config=jest.config.graphql.js",
    "test:jest:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:jest:watch": "jest --watch",
    "test:jest": "jest",
    "test:lockfile": "sh ./tasks/test-lockfile.sh",
    "test": "npm run test:jest && ./test-coverage.sh",
    "tslint-secondary": "echo 'Linting TypeScript...' && tslint --config ./tslint.secondary.json --format stylish --fix",
    "tslint": "echo 'Running tslint...' && tslint --config ./tslint.json --format stylish --project ./tsconfig.json",
    "perf:lighthouse-ci": "rimraf .lighthouseci && lhci autorun",
    "perf:lighthouse-ci:run": "pm2 delete all ; yarn build:local-assets && node ./tasks/lhci.ecosystem.config.js && yarn perf:lighthouse-ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn graphql && git add src/common/__generated__/api/*.ts && git diff --name-only --cached | yarn reorder && lint-staged && node ./test/no-date-hook",
      "pre-push": "commitlint --from=origin/master --to=HEAD"
    }
  },
  "lint-staged": {
    "*.@(j|t)s?(x|on)": [
      "yarn prettify",
      "git add"
    ],
    "*.ts?(x)": [
      "yarn tslint-secondary",
      "git add"
    ]
  },
